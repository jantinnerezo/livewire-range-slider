{
    "name": "jantinnerezo/livewire-range-slider",
    "description": "A Tall Stack wrapper for noUiSlider Range Slider",
    "keywords": [
        "jantinnerezo",
        "livewire-range-slider"
    ],
    "homepage": "https://github.com/jantinnerezo/livewire-range-slider",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jantinn Erezo",
            "email": "erezojantin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "livewire/livewire": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Jantinnerezo\\LivewireRangeSlider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jantinnerezo\\LivewireRangeSlider\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jantinnerezo\\LivewireRangeSlider\\LivewireRangeSliderServiceProvider"
            ],
            "aliases": {
                "LivewireRangeSlider": "Jantinnerezo\\LivewireRangeSlider\\LivewireRangeSliderFacade"
            }
        }
    }
}
